<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ydh.redsheep.base.mapper.UserMapper">

    <select id="list" resultType="user">
        SELECT * FROM user
    </select>

    <insert id="insert" >
        INSERT INTO user(username) VALUES (#{model.username})
    </insert>
    
</mapper>